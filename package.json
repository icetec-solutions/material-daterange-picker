{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@martindalec/material-daterange-picker",
  "description": "Angular Material Datepicker with Timepicker",
  "version": "1.1.3",
  "keywords": [
    "angular",
    "angular2",
    "ngx",
    "material",
    "angular-material",
    "datepicker",
    "daterange-picker",
    "material-daterange-picker"
  ],
  "author": "Chris Martindale",
  "homepage": "https://github.com/martindalec/material-datepicker",
  "license": "MIT",
  "repository": "github:martindalec/material-datepicker",
  "bugs": {
    "url": "https://github.com/martindalec/material-datepicker/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false --no-progress --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "rimraf dist && ng-packagr -p ng-package.json",
    "gh-build": "ng build --prod --output-path docs --base-href",
    "dist": "npm run dist:pkg && node ./scripts/dist.js",
    "dist:pkg": "ng build daterangepicker -c production",
    "dist:scss": "scss-bundle -e ./daterangepicker/src/lib/theming.scss -d ./dist/_theming.scss",
    "commitmsg": "commitlint -e",
    "preupload": "npm run dist",
    "upload": "npm publish --access=public dist/",
    "release": "standard-version --message 'build: release v%s'"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/core": "^0.4.2",
    "@angular/animations": "^6.0.4",
    "@angular/cdk": "^6.0.0",
    "@angular/cli": "~6.0.0",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/compiler-cli": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/language-service": "^6.0.4",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "@commitlint/cli": "^5.0.0",
    "@commitlint/config-angular": "^5.0.0",
    "@types/moment-timezone": "^0.5.4",
    "core-js": "^2.4.1",
    "cpx": "^1.5.0",
    "fs-extra": "^5.0.0",
    "moment": ">= 2.18.1",
    "moment-timezone": ">= 0.5.14",
    "ng-packagr": "^3.0.2",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.0.0-rc.0",
    "scss-bundle": "^2.1.2",
    "standard-version": "^4.3.0",
    "tsickle": "^0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.0",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  }
}